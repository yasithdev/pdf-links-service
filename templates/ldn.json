{
  "@context": ["https://www.w3.org/ns/activitystreams"],
  "type": ["Create"],
  "actor": {
    "id": "{{hostname}}",
    "type": "Service",
    "name":"PDF Links Service"
  },
  "object": {
    "type": "Document",
    "mediaType": "application/json",
    "name": "Robust Link Mappings for {{orig_pdf_name}}",
    "url": "{{hostname}}/mappings/{{pdf_name}}"
  },
  "origin": {
    "type": "Document",
    "mediaType": "application/pdf",
    "name": "{{orig_pdf_name}}",
    "url": "{{hostname}}/pdfs/{{pdf_name}}"
  },
  "summary": "PDF Links Service Created Robust Link Mappings for {{orig_pdf_name}}"
}